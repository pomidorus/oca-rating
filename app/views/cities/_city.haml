.b-city
  .rating
    = @city_rating.position_of city
  .title
    = link_to city.uk_title, city_path(city)
  .site_link
    .dot{title: 'Муниципальный сайт', class: "#{city.link?}"}
  .asset_disclosure
    = link_to city.asset_disclosure do
      .dot{title: 'Финансовые декларации', class: "#{city.asset_disclosure?}"}
  .budget
    = link_to city.budget_url do
      .dot{title: 'Бюджет', class: "#{city.budget?}"}
  .street_reestr
    .dot.false{title: 'Реестр названий улиц'}
